{
    "name": "boomdraw/laravel-canonicalizable",
    "description": "Canonicalizer package for Laravel",
    "keywords": [
        "boomdraw",
        "laravel",
        "trait",
        "eloquent",
        "canonicalize",
        "canonicalizer",
        "canonicalization",
        "canonicalizable"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/boomdraw/laravel-canonicalizable",
    "authors": [
        {
            "name": "Ivan Slesarenko",
            "email": "info@boomdraw.com",
            "homepage": "https://boomdraw.com",
            "role": "Owner"
        }
    ],
    "support": {
        "email": "info@boomdraw.com"
    },
    "require": {
        "php" : "^7.4",
        "boomdraw/canonicalizer": "^2",
        "illuminate/database": "^6.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Boomdraw\\Canonicalizable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boomdraw\\Canonicalizable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
